                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" textAlignment="justified" translatesAutoresizingMaskIntoConstraints="NO" id="2Jf-cJ-1QG">
                                <rect key="frame" x="0.0" y="382" width="414" height="279"/>
